{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/bin/fs2youtubemusic.js",
      "cwd": "${workspaceRoot}",
      "args": [
        "-p",
        ""
      ],
      "runtimeArgs": [
        "--harmony",
        "--no-deprecation"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Debug tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "args": [
        "test/**/*.spec.js"
      ],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector"
    }
  ]
}
